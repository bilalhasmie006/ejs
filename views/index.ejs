        <input type="checkbox">
        <p><%=html_items[i]%></p>

    </div>
    <%}%>
    <form action="/" method="post">
        <input type="text" name="newItem" value="" placeholder="New Item" autocomplete="off">
        <button type="submit" name="button" >+</button>
        
    </form> 


</div>


</body>
</html>